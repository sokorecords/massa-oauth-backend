{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/game/(.*)",
      "dest": "/api/game/$1"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
